<Window x:Class="HospitalManagementSystem.Views.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MoskIT" Width="400" Height="200" 
        ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="None" Foreground="{x:Null}" 
        Topmost="True"
        
        >
    <Window.Background >
        <ImageBrush ImageSource="/Launcher;component/Images/MoskitHome.png" Stretch="Fill" AlignmentX="Right"  AlignmentY="Center"></ImageBrush>
        
    </Window.Background>
        <StackPanel Background="Transparent">
            
          
            <StackPanel VerticalAlignment="Center" Margin="0,100,0,0">
                <StackPanel>
                    <ProgressBar Minimum="0" Maximum="100" x:Name="pbStatus" Height="10"  Width="200"/>
                <TextBlock Margin="100,0,0,0" x:Name="txtFileName" Foreground="White" FontSize="9" >Please Wait</TextBlock>
                </StackPanel>
            </StackPanel>
        </StackPanel>
    

    
</Window>
